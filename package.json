{
    "name": "tjs-vscode",
    "displayName": "TJS",
    "description": "Adding TJS language support for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "Biscrat",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sakano/tjs-vscode.git"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tjs",
            "aliases": ["TJS", "tjs"],
            "extensions": [".tjs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tjs",
            "scopeName": "source.tjs",
            "path": "./syntaxes/tjs.tmLanguage.json"
        }]
    }
}
