{
  "abstract function": {
    "prefix":"abstract",
    "body":[
      "AbstractFunc(${1:Name})($2)"
    ],
    "description": "Abstract function"
  },
  "class": {
    "prefix":"classi",
    "body":[
      "Class(${1:Name})",
      "{",
      "\tConstructor($2) {",
      "\t}",
      "}"
    ],
    "description": "Class"
  },
  "classe": {
    "prefix":"classei",
    "body":[
      "ClassExtends(${1:Name}, ${2:Parent})",
      "{",
      "\tConstructor($3) {",
      "\t}",
      "}"
    ],
    "description": "Child class"
  },
  "finalize": {
    "prefix":"finalizei",
    "body":[
      "Finalize() {",
        "\t$0",
      "}"
    ],
    "description": "Finalize method"
  },
  "lambda": {
    "prefix":"lmd",
    "body":[
      "LMD($1) {",
      "\t$0",
      "}"
    ],
    "description": "lambda"
  },
  "function": {
    "prefix":"funci",
    "body":[
      "Func(${1:Name})($2) {",
      "\t$0",
      "}"
    ],
    "description": "Function"
  },
  "override function": {
    "prefix":"override",
    "body":[
      "OverrideFunc(${1:Name})($2) {",
      "\t$0",
      "}"
    ],
    "description": "Override function"
  }
}
